[
 {gproc, [{gproc_dist, all}]},
{redgrid,
  [
   {url, "_REDIS_URL_"},
   {meta, []},
   {anonymous, false},
   {domain, "_REDIS_DOMAIN_"},
   {version, ""},
   {local_ip, "_OWN_IP_"}
  ]},
 {sasl, [
         {sasl_error_logger, {file, "/var/log/wiggle/error.log"}},
         {errlog_type, error},
         {error_logger_mf_dir, "/var/log/wiggle"},      % Log directory
         {error_logger_mf_maxbytes, 10485760},   % 10 MB max file size
         {error_logger_mf_maxfiles, 5}           % 5 files max
        ]},
 {wiggle, [
           {acceptors, 100},
           {port, 80},
           {key_id, "wiggle"},
           {session_name, <<"wiggle">>},
           {session_key, <<"0123456789ABCDEF">>}
          ]},
 {alog, [
	 {enabled_loggers, [alog_tty,alog_disk_log]},
	 {flows, [
		  {{mod,['_']}, {'=<', warning}, [alog_tty, alog_disk_log]}
		 ]}
	]}
].
